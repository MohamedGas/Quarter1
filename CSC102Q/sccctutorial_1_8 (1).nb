Cell[TextData[{
 "Note that in general ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SqrtBox[
     SuperscriptBox["n", "2"]], "=", "n"}], TraditionalForm]],ExpressionUUID->
  "d0ae81d4-fb16-400e-b911-7701726e595b"],
