   3.396366936*^9, 3.396367002109375*^9}, {3.3963671671875*^9, 
   3.396367194046875*^9}, {3.396367226171875*^9, 3.396367351109375*^9}, {
   3.396370322375*^9, 3.396370351765625*^9}, {3.396370744015625*^9, 
