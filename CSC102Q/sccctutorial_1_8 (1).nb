   3.394558827589375*^9}, {3.394559854339375*^9, 3.394559947245625*^9}, 
   3.3948255147991247`*^9, {3.39632064325*^9, 3.396320659203125*^9}, {
   3.396363325*^9, 3.39636335534375*^9}, {3.396364585796875*^9, 
   3.3963645875625*^9}, {3.396366420328125*^9, 3.396366530984375*^9}, {
   3.396366622421875*^9, 3.396366635921875*^9}, {3.3963706295625*^9, 
   3.396370660921875*^9}, {3.396374443546875*^9, 3.39637448240625*^9}, {
   3.396374549046875*^9, 3.396374549609375*^9}, {3.3963757960625*^9, 
