  FormBox[
   RowBox[{"y", "=", 
    RowBox[{"5", "-", 
     FractionBox[
      SuperscriptBox["k", "2"], "4"]}]}], TraditionalForm]],ExpressionUUID->
  "efd7c413-76bf-4a83-a705-1d52fb8e8709"],
 " , the ",
 StyleBox["y-",
  FontSlant->"Italic"],
 "coordinate ",
 StyleBox["decreases",
  FontSlant->"Italic"],
 " as ",
 StyleBox["k",
  FontSlant->"Italic"],
