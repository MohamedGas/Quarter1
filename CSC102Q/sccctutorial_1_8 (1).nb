Cell[BoxData[
 RowBox[{"ourequation", " ", "=", " ", 
  RowBox[{
   RowBox[{
    RowBox[{"Cos", "[", "\[Theta]", "]"}], " ", "==", " ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"a", "^", "2"}], " ", "+", " ", 
       RowBox[{"b", "^", "2"}], " ", "-", " ", 
