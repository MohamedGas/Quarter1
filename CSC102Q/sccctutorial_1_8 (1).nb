Cell[TextData[{
 StyleBox["Variables can be represented by single letters or multiple \
characters.  For example ",
  CellFrame->0.5],
 Cell[BoxData[
  FormBox[
   RowBox[{
   "x", ",", " ", "y", ",", " ", "Y", ",", " ", "cost", ",", " ", "speed", 
    ",", " ", 
    RowBox[{"and", " ", "Profit"}], " ", ","}], TraditionalForm]],
  ExpressionUUID->"70f35c85-7a55-4346-9eef-84682ceea148"],
 " ",
 StyleBox["are all acceptable names for variables in ",
  CellFrame->0.5],
 StyleBox["Mathematica",
  CellFrame->0.5,
  FontSlant->"Italic"],
 StyleBox[" . Keep in mind that ",
  CellFrame->0.5],
 StyleBox["Mathematica",
  CellFrame->0.5,
  FontSlant->"Italic"],
 StyleBox[" is case sensitive, so it will treat ",
  CellFrame->0.5],
 Cell[BoxData[
  FormBox[
   RowBox[{"x", " ", "and", " ", "X", " "}], TraditionalForm]],
  ExpressionUUID->"3d7aa415-4fe2-414b-8e74-cd2427256275"],
 " ",
 StyleBox["as two different variables. The following rules will help you \
construct algebraic expressions. ",
  CellFrame->0.5]
}], "Text",
 CellFrame->None,
 CellFrameColor->Automatic,
 CellChangeTimes->{{3.3936943905508256`*^9, 3.393694402608163*^9}, {
