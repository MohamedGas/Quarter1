Cell[TextData[{
 StyleBox["Global assignment (",
  CellFrame->{{0.5, 3}, {3, 0.5}},
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", "value"}], TraditionalForm]],
  FontWeight->"Bold",ExpressionUUID->"9e69264c-099d-4ed9-a318-043cb437ad90"],
 StyleBox[") vs. local substitution (",
  CellFrame->{{0.5, 3}, {3, 0.5}},
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   RowBox[{"/.", 
    RowBox[{"x", "\[Rule]", "value"}]}], TraditionalForm]],
  FontWeight->"Bold",ExpressionUUID->"6eb3503b-941f-4839-beab-e99c86e83a08"],
 StyleBox[")",
  FontWeight->"Bold"],
 StyleBox["\nHere is an important distinction to keep in mind. \nWhen we \
enter x = 5 we are directing ",
  CellFrame->{{0.5, 3}, {3, 0.5}}],
 StyleBox["Mathematica",
  CellFrame->{{0.5, 3}, {3, 0.5}},
  FontSlant->"Italic"],
 StyleBox[" to ",
  CellFrame->{{0.5, 3}, {3, 0.5}}],
 StyleBox["assign",
  CellFrame->{{0.5, 3}, {3, 0.5}},
  FontVariations->{"Underline"->True}],
 StyleBox[" the value of 5 to x.  From that point on, x has the value of 5 \
anywhere and everywhere it appears in the notebook.\n\nIn contrast, when we \
enter, say, ",
  CellFrame->{{0.5, 3}, {3, 0.5}}],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["x", "2"], "+", "4"}], " ", "/.", " ", 
    RowBox[{"x", "\[Rule]", " ", "5"}]}], TraditionalForm]],ExpressionUUID->
  "695878ff-f3a2-4556-8c03-0659c0c8703b"],
 ", we are directing ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " to ",
 StyleBox["replace",
  FontVariations->{"Underline"->True}],
 " the x in ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "2"], "+", "4"}], TraditionalForm]],ExpressionUUID->
  "f874ac0b-9d61-45f2-a0e9-e3bc62ff2fbe"],
 " by 5, BUT, ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "1bbbaece-2b57-43f5-8651-9e237b1e1823"],
 " itself ",
 StyleBox["does not change",
  FontVariations->{"Underline"->True}],
 " ; ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "bf719f8c-79db-45f5-bee8-45503462ee37"],
 " remains a symbolic variable with no particular value. "
}], "Text",
