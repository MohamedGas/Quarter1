     RowBox[{
      RowBox[{
       SuperscriptBox["x", "2"], "-", 
       RowBox[{"5", "x"}], "+", "6"}], "\[Equal]", "0"}], ",", "x"}], "]"}], 
   TraditionalForm]],ExpressionUUID->"4752a0d9-c76a-49a8-a864-cf5203e0b791"],
