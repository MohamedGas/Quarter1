Cell[TextData[{
 StyleBox["Standard vs. Traditional Form\[LineSeparator]",
  CellFrame->0.5,
  FontWeight->"Bold",
  FontVariations->{"Underline"->True}],
 StyleBox["Mathematica",
  CellFrame->0.5,
  FontSlant->"Italic"],
 StyleBox["  has two formats available for displaying algebraic output. They \
are called ",
  CellFrame->0.5],
 StyleBox["StandardForm",
  CellFrame->0.5,
  FontWeight->"Bold"],
 StyleBox[" and ",
  CellFrame->0.5],
 StyleBox["TraditionalForm",
  CellFrame->0.5,
  FontWeight->"Bold"],
 StyleBox[". The default form is StandardForm. The most noticeable aspect of \
this form is that polynomial expressions are displayed in ",
  CellFrame->0.5],
 StyleBox["ascending",
  CellFrame->0.5,
  FontSlant->"Italic"],
 StyleBox[" powers of the variable. So if you enter ",
  CellFrame->0.5],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "2"], "+", 
    RowBox[{"3", "x"}], "+", "5"}], TraditionalForm]],ExpressionUUID->
  "a0fa5f13-7852-4b7a-987c-131867836ec0"],
 ", ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " will automatically display it as ",
 Cell[BoxData[
  FormBox[
   RowBox[{"5", "+", 
    RowBox[{"3", "x"}], "+", 
    SuperscriptBox["x", "2"]}], TraditionalForm]],ExpressionUUID->
  "55282d8b-5312-418d-8d0a-aecd5b9668d5"],
 ". Execute the line below to see how this works. "
}], "Text",
