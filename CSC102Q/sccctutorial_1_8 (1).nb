Cell[TextData[{
 StyleBox["Parentheses vs.Square Brackets",
  CellFrame->0.5,
  FontWeight->"Bold",
  FontVariations->{"Underline"->True}],
 StyleBox["\nYou have seen that square brackets are used to enclose input \
arguments in both commands and functions. This is the only place in ",
  CellFrame->0.5],
 StyleBox["Mathematica",
  CellFrame->0.5,
  FontSlant->"Italic"],
 StyleBox[" where square brackets are allowed. Therefore, when entering a \
mathematical expression ",
  CellFrame->0.5],
 StyleBox["you must use parentheses for all grouping",
  CellFrame->0.5,
  FontVariations->{"Underline"->True}],
 StyleBox[". You cannot use square brackets for grouping the way you do when \
writing math by hand. So, for example, in ",
  CellFrame->0.5],
 StyleBox["Mathematica",
  CellFrame->0.5,
  FontSlant->"Italic"],
 StyleBox["  ",
  CellFrame->0.5],
 Cell[BoxData[
  FormBox[
   RowBox[{"5", 
    RowBox[{"(", 
     RowBox[{"x", "+", 
      RowBox[{"3", 
       RowBox[{"(", 
        RowBox[{"x", "-", "2"}], ")"}]}]}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"ca0fde6a-4250-4730-9bd9-d165eb85a099"],
 " ",
 StyleBox["cannot",
  FontVariations->{"Underline"->True}],
 " be entered as ",
 StyleBox[" ",
  CellFrame->0.5],
 Cell[BoxData[
  FormBox[
   RowBox[{"5", " ", "[", 
    RowBox[{"x", "+", 
     RowBox[{"3", 
      RowBox[{"(", 
       RowBox[{"x", "-", "2"}], ")"}]}]}], "]"}], TraditionalForm]],
  ExpressionUUID->"ebc217d4-6fcb-4387-bf61-59a0fd6defc9"],
 " .",
 StyleBox[" ",
  CellFrame->0.5]
}], "Text",
